{
  "name": "@tjsr/lwdb",
  "version": "0.0.1",
  "description": "Last War DB",
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "test": "vitest --run && npm run lint",
    "build": "tsc",
    "start:dev": "nodemon",
    "start:prod": "node index.js",
    "clean": "npx rimraf dist build",
    "clean:all": "npx rimraf dist build node_modules",
    "lint": "eslint --config eslint.config.mjs index.ts src/*.ts",
    "lint:fix": "eslint --fix src/*.ts",
    "prepack": "npm test && npm run build:dist",
    "prepublishOnly": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjsr/lwdb.git"
  },
  "keywords": [
    "Express"
  ],
  "author": "Tim Rowe <tim@tjsr.id.au>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tjsr/lwdb/issues"
  },
  "homepage": "https://github.com/tjsr/lwdb#readme",
  "devDependencies": {
    "@tjsr/eslint-config": "github:tjsr/eslint-config#main",
    "@tjsr/express-server-helper": "^0.0.3",
    "@tjsr/simple-env-utils": "^0.1.3",
    "@tjsr/user-session-middleware": "^0.0.36",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/node": "^20.14.14",
    "@types/session-file-store": "^1.2.5",
    "@types/source-map-support": "^0.5.10",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.5",
    "typescript": "^5.5.4",
    "vitest": "^1.6.0"
  },
  "overrides": {
    "rimraf@<=4": "$rimraf"
  },
  "dependencies": {
    "@tjsr/fs-utils": "^0.0.6",
    "express-session": "^1.18.0",
    "session-file-store": "^1.5.0",
    "source-map-support": "^0.5.21"
  }
}
