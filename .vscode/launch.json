{
  "configurations": [
    {
      "name": "TSX: Run LWDB as server",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true,
      "type": "node",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "tsx"
      ],
      "env": {
        "DOTENV_FLOW_PATTERN": ".env.development[.local]"
      },
      "args": [
        "${workspaceFolder}/src/index.ts"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "tsc: Run LWDB as server",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "sourceMaps": true,
      "type": "node",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "tsx"
      ],
      "program": "${workspaceFolder}/src/index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "env": {
        "DOTENV_FLOW_PATTERN": ".env.development[.local]"
      },
      "outFiles": [
        "${workspaceFolder}/dist/esm/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Run as server (2)",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Attach to server",
      "port": 8999,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },{
      "type": "node",
      "request": "launch",
      "name": "Debug xls2csv",
      "program": "${workspaceFolder}/bin/xls2csv",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    },{
      "type": "node",
      "request": "launch",
      "name": "Debug xls2json",
      "program": "${workspaceFolder}/bin/xls2json",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "skipFiles": ["<node_internals>/**"],
      "console": "integratedTerminal"
    }
  ]
}